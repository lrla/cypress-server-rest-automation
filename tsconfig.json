{
    "compileOnSave":false,
    "compilerOptions":{
       "allowSyntheticDefaultImports":true,
       "baseUrl":"cypress",
       "paths":{
          "*":[
             "*"
          ]
       },
       "incremental":true,
       "declaration":true,
       "sourceMap":true,
       "alwaysStrict":true,
       "noImplicitAny":true,
       "noUnusedLocals":true,
       "removeComments":false,
       "preserveConstEnums":true,
       "strictNullChecks":true,
       "strict":true,
       "downlevelIteration":true,
       "experimentalDecorators":true,
       "esModuleInterop":true,
       "importHelpers":true,
       "module":"commonjs",
       "target":"ES6",
       "lib":[
          "esnext",
          "dom"
       ],
       "types":[
          "cypress",
          "node",
          "cypress-xpath",
          "cypress-grep",
          "cypress-file-upload"
       ]
    }
 }